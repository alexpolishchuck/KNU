#include <iostream>
#include<math.h>
#include <clocale>
#include "LibArray.h";

using std::cout;
using std::cin;
using std::endl;


int main()
{
	
	
		int x, y, z,N;
		cout << "Enter N";
		cin >> N;

		for (x = -N; x < N; x++)
		{
			for (y = -N; y < N; y++)
			{
				for (z = -N; z <= N; z++)
				{
					if (pow(x, 2) + pow(y, 2) == pow(z, 2) && x<y && y<z) {

						cout << x << " " << y << " " << z << endl;
					}
				}
			}
		}
		return 0;
	
}
