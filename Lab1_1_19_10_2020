#include <iostream>
#include <math.h>
using std::endl;
using std::cout;
using std::cin;

//	Знайти всі трійки чисел x<y<z≤N такі, що x^2+y^2=z^2. 
int main()
{
	int x, y, z;
	for (x = -100; x <= 100; x++)
		for (y = -100; y <= 100; y++)
			for (z = -100; z <= 100; z++)
				if (pow(x, 2) + pow(y, 2) == pow(z, 2))
					cout << x << " " << y << " " << z << endl;
}
