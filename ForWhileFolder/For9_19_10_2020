//головний код 
#include <iostream>
#include<math.h>
#include <clocale>
#include "Libfor.h"
#include <conio.h>
using std::cout;
using std::cin;
using std::endl;





int main()
{
	int A, B;
	cout << "Enter A>B coherently :";
	cin >> A >> B;
	cout << endl;
	square(A, B);
	return 0;
};
//функція в Libfor.cpp
void square(int A, int B) {
	for (int i = A; i <= B; i++) {
		A = i * i;
		cout << A << endl;
	}
};
//заголовок в Libfor.h
void square(int A, int B);

