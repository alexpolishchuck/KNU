#include <iostream>
#include<math.h>
#include <clocale>


using std::cout;
using std::cin;
using std::endl;

	


int main()
{
	
	setlocale(LC_ALL, "Ukrainian_Ukraine.1251");
	int a, b, c, d;
	cin >> a >> b >> c >> d;
	if (a < b && a < c && a < d)
		cout << a;
	else
		if (b < a && b < c && b<d)
			cout << b;
		else if (c < b && c < a && c<d)
			cout << c;
			if (d < a && d < c && d < b)
		cout << d;
	
	
	return 0;
}
