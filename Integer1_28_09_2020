#include <iostream>
#include <stdio.h>
#include <locale>
using std::cout;
using std::cin;
using std::endl;
    int main()
    {
        setlocale(LC_CTYPE, "ukr");
        float L;
        cout << "Введiть значення L в сантиметрах L=";
        cin >> L ;
        cout << endl;
        L = L/100;

        cout<<"Довжина в метрах : " << L;
        return 0;
    }
